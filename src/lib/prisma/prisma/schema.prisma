// Setup
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Model
model trending {
  id              String   @id @default(uuid()) @db.Uuid
  title           String
  url             String
  data            Json?
  typeTrending    String
  createdAt       DateTime @default(now())
}
